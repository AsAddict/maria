{
  "hosting": {
    "public": "resources/public",
    "ignore": [
      "js/compiled/out**"
    ],
    "rewrites": [
      {
        "source": "/js/**",
        "destination": "404.html"
      },
      {
        "source": "**",
        "destination": "/index.html",
        "type": 302
      }
    ]
  }
}
