{
  "hosting": {
    "public": "public",
    "ignore": [
      "js/compiled/out**",
      "js/compiled/cljs-runtime/**",
      "js/compiled/node_modules/**",
      "js/compiled/bootstrap/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
